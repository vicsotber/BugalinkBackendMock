{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
  { "src": "/", "dest": "/index.js" },
  { "src": "/api/users/:userId", "dest": "/index.js" },
  { "src": "/api/users/:driverId/reviews", "dest": "/index.js" },
  { "src": "/users/:userId/chats/pending/count", "dest": "/index.js" },
  { "src": "/users/:userId/requests/pending/count", "dest": "/index.js" },
  { "src": "/trips/recommendations", "dest": "/index.js" },
  { "src": "/individualRides/:individualRideId", "dest": "/index.js" },
  { "src": "/trips/search", "dest": "/index.js" },
  { "src": "/users/:userId/trips", "dest": "/index.js" },
  { "src": "/users/:userId/trips/upcoming", "dest": "/index.js" },
  { "src": "/users/:userId/trips/history", "dest": "/index.js" },
  { "src": "/users/:userId/routines", "dest": "/index.js" }
  ],
  "env": {
    "TS_NODE_PROJECT": "./tsconfig.json"
  }
}
